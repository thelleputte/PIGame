	<script src="dist/js/jquery.js"></script>
	<script>
		function timer(n) {
			var rand = randomIntFromInterval(1, 5)
			//$(".progress-bar").css("width", n + "%");
			$("#" + rand).css("width", n + "%");
			$("#pourcentage").text(n + "%");
			if (n < 100) {
				setTimeout(function() {
					timer(n + 10);
				}, 200);
			}
		}
		function randomIntFromInterval(min, max) {
			return Math.floor(Math.random() * (max - min + 1) + min);
		}
		$(function() {
			$("#animer").click(function() {
				timer(0);
			});
		});
	</script>

